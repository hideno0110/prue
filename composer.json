{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "hesto/multi-auth": "^1.0",
        "laravelcollective/html": "^5.3",
        "kyslik/column-sortable": "^5.1",
        "fzaninotto/faker": "^1.6",
        "alexpechkarev/google-maps": "1.0.5",
        "acacha/admin-lte-template-laravel": "3.*",
        "weidner/goutte": "@dev",
        "peron/laravel5-amazon-mws": "^2.0",
        "s-ichikawa/laravel-sendgrid-driver": "^1.1",
        "bogardo/mailgun": "^5.0",
        "guzzlehttp/guzzle": "^6.2",
        "modelizer/selenium": "~1.0",
        "roumen/sitemap": "^2.6",
        "davejamesmiller/laravel-breadcrumbs": "^3.0",
        "prettus/l5-repository": "^2.6"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
